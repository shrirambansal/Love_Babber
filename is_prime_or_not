#include <iostream>
using namespace std;
bool isprime(int n) {
    for (int i =2; i<n; i++){
        if(n%i==0) {
            return 0;
        }
    }
    return 1;
}

int main() {
    int num;
    cin >> num;
    int ans = isprime(num);
    if (ans == 1) {
        cout << "Number is Prime";
    }
    else {
        cout << "Number is not Prime";
    }
}
